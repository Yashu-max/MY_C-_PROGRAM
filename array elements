#include<stdio.h>
#include<stdlib.h>
int main()
{
 int n;
 printf("enter size:");
 scanf("%d",&n);
 int* arr=(int*) malloc (10*sizeof (int));
 for(int i=0;i<5;i++){
     printf("enter elements %d:",i++);
     scanf("%d",&arr[i]);
 }
 printf("array elements :\n");
 for(int i=0;i<5;i++){
     printf("%d",arr[i]);
 }
 free(arr);
 return 0;
}
