#include<stdio.h>
enum oper{
    add=1,
    sub,
    multi,
    divi
};
int main(){
   enum oper op;
   printf("enter  a oper(1-4)\n");
   scanf("%d",&op);
   if(op==add){
       printf("addation\n");
   } else if(op==sub){
       printf("subtraction");
   }
   else if(op==multi){
       printf("multipacation");
   }
   else{
       printf("other oper");
   }
   
}
