#include <stdio.h>
struct student{
    char name[70];
    int roll;
    float marks;
};
int main(){
struct student s1;
printf("enter name:");
scanf("%s",s1. name);
printf("enter roll number");
scanf("%d",&s1. roll);
printf("enter the marks");
scanf("%f,&s1",&s1 .marks);
printf("nstudent datails;n/");
printf(" the detsils of students is: %s,%d,%f",s1.name,s1.roll,s1.marks);
}
